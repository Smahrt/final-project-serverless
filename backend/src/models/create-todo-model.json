{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "todo",
  "type": "object",
  "properties": {
    "name": {
      "type": "string"
    },
    "userId": {
      "type": "string"
    },
    "dueDate": {
      "type": "string"
    },
    "attachmentUrl": {
      "type": "string"
    },
    "done": {
      "type": "boolean"
    },
    "createdAt": {
      "type": "string"
    },
    "todoId": {
      "type": "string"
    },
    "updatedAt": {
      "type": "string"
    }
  },
  "required": [
    "name",
    "userId"
  ]
}